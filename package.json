{
  "name": "fapinstructor-client",
  "version": "2.0.1",
  "private": false,
  "homepage": "https://fapinstructor.com/",
  "license": "MIT",
  "prepare": "npx husky install",
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test --env=jsdom",
    "test:ci": "CI=true yarn test --bail --findRelatedTests",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "lint": "eslint --ext .js,.ts,.tsx,.snap ./src --ignore-path .gitignore --max-warnings=0 .",
    "prettier": "prettier \"**/*.+(js|ts|tsx|json|yml|md)\"",
    "format": "yarn prettier --write",
    "check-format": "yarn prettier --list-different",
    "check-types": "tsc --project tsconfig.json --pretty --noEmit",
    "validate": "CI=true npm-run-all --parallel check-types test check-format lint",
    "chromatic": "chromatic --exit-zero-on-changes",
    "bundle-analyzer": "cra-bundle-analyzer",
    "generate": "plop"
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@craco/craco": "^6.4.0",
    "@material-ui/core": "^4.12.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "4.0.0-alpha.60",
    "@reduxjs/toolkit": "^1.6.1",
    "@sentry/react": "^6.10.0",
    "@sentry/tracing": "^6.10.0",
    "@xstate/inspect": "^0.4.1",
    "@xstate/react": "^1.5.1",
    "axios": "^0.21.1",
    "date-fns": "^2.23.0",
    "formik": "^2.2.9",
    "formik-material-ui": "^3.0.1",
    "github-markdown-css": "^4.0.0",
    "lodash": "^4.17.21",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-markdown": "^6.0.2",
    "react-query": "^3.32.1",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "standardized-audio-context": "^25.3.0",
    "styled-components": "^5.3.0",
    "typescript": "^4.3.5",
    "xstate": "^4.23.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@storybook/addon-actions": "^6.3.6",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/addon-storyshots": "^6.3.6",
    "@storybook/node-logger": "^6.3.6",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.6",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/faker": "^5.5.7",
    "@types/jest": "^26.0.24",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/lodash": "^4.14.172",
    "@types/node": "^16.4.3",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/react-helmet": "^6.1.4",
    "@types/react-redux": "^7.1.18",
    "@types/react-router-dom": "^5.1.8",
    "@types/styled-components": "^5.1.11",
    "@types/webpack-env": "^1.16.0",
    "@types/yup": "^0.29.13",
    "babel-plugin-polyfill-corejs2": "^0.2.2",
    "babel-plugin-polyfill-regenerator": "^0.2.2",
    "chromatic": "^5.9.2",
    "cra-bundle-analyzer": "^0.1.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-testing-library": "^4.10.1",
    "faker": "^5.5.3",
    "husky": "^7.0.1",
    "jest-fetch-mock": "^3.0.3",
    "jest-styled-components": "^7.0.5",
    "lint-staged": "^11.1.1",
    "mockdate": "^3.0.5",
    "msw": "^0.33.0",
    "npm-run-all": "^4.1.5",
    "plop": "^2.7.6",
    "prettier": "^2.3.2",
    "react-is": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "storybook-preset-craco": "^0.0.6"
  }
}
